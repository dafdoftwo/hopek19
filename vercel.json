{
  "version": 2,
  "builds": [
    {
      "src": "api/orders.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/orders",
      "dest": "/api/orders.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/orders.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
