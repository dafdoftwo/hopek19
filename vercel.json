{
  "version": 2,
  "builds": [
    {
      "src": "api/orders.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "confirm.html",
      "use": "@vercel/static"
    },
    {
      "src": "media/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/orders",
      "dest": "/api/orders.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/orders.js"
    },
    {
      "src": "/confirm",
      "dest": "/confirm.html"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
